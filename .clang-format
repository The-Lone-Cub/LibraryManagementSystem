BasedOnStyle: LLVM
IndentWidth: 4
ColumnLimit: 0
AllowShortFunctionsOnASingleLine: Empty
BreakBeforeBraces: Custom
BraceWrapping:
  AfterFunction: false
  AfterClass: false
  AfterControlStatement: false
  AfterEnum: false
  AfterNamespace: false
  AfterStruct: false
  BeforeCatch: false
  BeforeElse: false
  IndentBraces: false
AlignConsecutiveAssignments: false
AlignConsecutiveDeclarations: false
ReflowComments: false
SortIncludes: false
